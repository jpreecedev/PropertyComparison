{
  "name": "propertycomparison",
  "version": "1.0.0",
  "description": "A useful tool for comparing properties against each other and your property investment goals",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/report"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpreecedev/PropertyComparison.git"
  },
  "author": "Jon Preece",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jpreecedev/PropertyComparison/issues"
  },
  "homepage": "https://github.com/jpreecedev/PropertyComparison#readme",
  "dependencies": {
    "chota": "^0.4.2",
    "classnames": "^2.2.5",
    "next": "^4.2.3",
    "next-redux-wrapper": "^1.3.5",
    "normalize.css": "^7.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.2.1",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "cypress": "^1.4.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mocha": "^4.0.1",
    "mochawesome": "^1.5.5",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.10",
    "postcss-modules": "^1.1.0",
    "react-css-modules": "^4.7.1",
    "redux-devtools-extension": "^2.13.2",
    "react-test-renderer": "^16.2.0",
    "skeleton-loader": "^1.1.4",
    "style-loader": "^0.19.1",
    "trash": "^4.2.1",
    "webpack": "^3.10.0"
  }
}
